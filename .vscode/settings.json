{
  "files.autoSave": "off",
  "editor.formatOnSave": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "prettier.singleQuote": true,
  "files.exclude": {
    "**/.gitignore": true,
    "**/.git": true,
    "**/.terraform": true,
    "**/.tfstate": true,
    "**/.npmrc": true,
    "**/dev/dvc-command": true,
    "**/.env.backup": true,
    "**/.dockerignore": true,
    "**/dvc-command": true,
    "**/gha**": true,
    "**/package-lock.json": true,
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/.next": true,
    "**/dist": true,
    "**/.terraform.lock.hcl": true,
    "**/backend.tf": true,
    "**/versions.tf": true,
    "**/.npmignore": true,
    "**/.env": true
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.cacheOnSave": true,
  "deno.path": "/deno/bin/deno",
  "deno.cache": "/deno/.cache",
  "deno.config": "/home/vscode/deno.json",
  "vsicons.associations.folders": [
    {
      "icon": "app",
      "extensions": [
        "potion"
      ],
      "format": "svg"
    },
    {
      "icon": "log",
      "extensions": [
        "scripts"
      ],
      "format": "svg"
    },
    {
      "icon": "buildkite",
      "extensions": [
        "state"
      ],
      "format": "svg"
    },
    {
      "icon": "audio",
      "extensions": [
        "dev"
      ],
      "format": "svg"
    },
    {
      "icon": "bloc",
      "extensions": [
        "ui"
      ],
      "format": "svg"
    },
    {
      "icon": "docker",
      "extensions": [
        "container"
      ],
      "format": "svg"
    },
    {
      "icon": "e2e",
      "extensions": [
        "workflows"
      ],
      "format": "svg"
    },
    {
      "icon": "cubit",
      "extensions": [
        "library-scripts"
      ],
      "format": "svg"
    }
  ],
  "vsicons.associations.files": [
    {
      "icon": "config",
      "extensions": [
        ".env.local"
      ],
      "format": "svg"
    },
    {
      "icon": "config",
      "extensions": [
        ".env.template"
      ],
      "format": "svg"
    },
    {
      "icon": "pytyped",
      "extensions": [
        ".toml"
      ],
      "format": "svg"
    }
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "**/meta.json"
      ],
      "url": "https://raw.githubusercontent.com/ghostmind-dev/config/main/config/meta/schema.json"
    }
  ]
}